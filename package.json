{
  "name": "@sxzz/eslint-config",
  "version": "4.2.0",
  "packageManager": "pnpm@9.11.0",
  "description": "ESLint config for @sxzz.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/sxzz/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/eslint-config.git"
  },
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "build": "pnpm run build:typegen && tsup",
    "build:typegen": "tsx scripts/typegen.ts",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@eslint/js": "^9.11.1",
    "@eslint/markdown": "^6.1.1",
    "@unocss/eslint-plugin": "^0.63.1",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-antfu": "^2.7.0",
    "eslint-plugin-command": "^0.2.6",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.3.1",
    "eslint-plugin-jsdoc": "^50.3.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-perfectionist": "^3.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-sxzz": "^0.1.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-yml": "^1.14.0",
    "globals": "^15.9.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.7.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.4",
    "@sxzz/prettier-config": "^2.0.2",
    "@types/node": "^22.7.4",
    "bumpp": "^9.6.1",
    "eslint": "^9.11.1",
    "eslint-typegen": "^0.3.2",
    "importx": "^0.5.0",
    "picocolors": "^1.1.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@no-shims/is-core-module"
    }
  },
  "prettier": "@sxzz/prettier-config"
}
